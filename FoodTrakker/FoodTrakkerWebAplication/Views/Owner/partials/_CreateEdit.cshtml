@model FoodTruck


<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<div class="form-group">
	<label asp-for="Name" class="control-label"></label>
	<input asp-for="Name" class="form-control" />
	<span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group">
	<label asp-for="Description" class="control-label"></label>
	<input asp-for="Description" class="form-control" />
	<span asp-validation-for="Description" class="text-danger"></span>
</div>
<div class="form-group">
	<label asp-for="Type.Name" class="control-label"></label>
	<input asp-for="Type.Name" class="form-control" />
	<span asp-validation-for="Type.Name" class="text-danger"></span>
</div>
<div class="form-group">
	<label asp-for="Location.City" class="control-label"></label>
	<input asp-for="Location.City" class="form-control" />
	<span asp-validation-for="Location.City" class="text-danger"></span>
</div>
<div class="form-group">
	<label asp-for="Location.Street" class="control-label"></label>
	<input asp-for="Location.Street" class="form-control" />
	<span asp-validation-for="Location.Street" class="text-danger"></span>
</div>
<hr/>
